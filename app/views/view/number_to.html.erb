
1:::<%= number_to_currency(12345) %>
<br><br>

2:::<%= number_to_currency(12345, unit: '円', precision: 0, format: '%n%u') %>
<br><br>

3:::<%= number_to_human(1234567) %>
<br><br>

4:::<%= number_to_human(1234567, units: { unit: "", million: "百万" }) %>
<br><br>

5:::<%= number_to_human_size(123456) %>
<br><br>

6:::<%= number_to_percentage(12.3456) %>
<br><br>

7:::<%= number_with_delimiter(1234567.89, delimiter: " ", separator: ",") %>
<br><br>

8:::<%= number_with_precision(123.4567) %>
<br><br>

9:::<%= number_with_precision(123.4567, precision: 5) %>
<br><br>

10:::<%= number_with_precision(123.4567, precision: 5, significant: true) %>
<br><br>

11:::<%= number_with_precision(123.4, precision: 5, strip_insignificant_zeros: true) %>


